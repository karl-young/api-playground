{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "react-jsx",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
   
  },
  "include": ["client","models", "server"],
  "exclude": ["node_modules", "dist"]
}
